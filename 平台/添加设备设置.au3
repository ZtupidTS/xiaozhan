#include <GuiListView.au3>


HotKeySet("{ESC}", "Terminate")
HotKeySet("{F2}", "Togglepause")

Local $j =10001 , $Paused 
$i = 0
Do

WinActivate("锐眼(RaySee) -- 管理客户端")
$hListView1 = ControlGetHandle ( "锐眼(RaySee) -- 管理客户端", "List1", "SysListView3223")
_GUICtrlListView_ClickItem($hListView1, 1, "right", True, 1)
Send("{down}")
Send("{enter}")
WinWait("业务添加窗口","","Edit2")

WinActivate("业务添加窗口")
ControlClick("业务添加窗口","","Edit2")
Send($j)
$hListView = ControlGetHandle ( "业务添加窗口", "List1", "SysListView321")
 _GUICtrlListView_SetItemChecked($hListView, 0);列表框里面的checkbox勾选方法
 ControlClick("业务添加窗口","确定","Button1")
 
 $j=$j+1
 
  $i = $i + 1
Until $i = 5

Func Terminate()
	Exit 0
EndFunc

Func Togglepause()
    $Paused = NOT $Paused 
	
   While $Paused 
	tooltip("暂停一会",0,0)
	sleep(100)
   tooltip("")
   WEnd
EndFunc
