HotKeySet("{F4}", "Terminate")
HotKeySet("{F2}", "Togglepause")

Local $Paused, $num =1

$i = 0
Do


If $num <=10 Then
	dj()
    Sen($num)
	$num=$num+1
else
   $num = 1
EndIf


$i = $i + 1
Until $i = 100000000




Func Terminate()
	Exit 0
EndFunc

Func Togglepause()
    $Paused = NOT $Paused 
	
   While $Paused 
	tooltip("暂停一会",0,0)
	sleep(100)
   tooltip("")
   WEnd
EndFunc


func  sen($num)
for $k = 1 to $num
    Send("{down}")
	
	Sleep(1000)
	;Send("{enter}")
    ;Sleep(5*1000)
next
endfunc

func dj()
    WinActivate("视频房间(小站)")
    ControlClick("视频房间(小站)","彩字","Button60")
endfunc


